<div class="occurrence-performance">
    {% if row.memoryUsage %}
        <div class="occurrence-performance__memory">
            <svg width="12" height="12" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 3v2m6-2v2M9 19v2m6-2v2M5 9H3m2 6H3m18-6h-2m2 6h-2M7 19h10a2 2 0 002-2V7a2 2 0 00-2-2H7a2 2 0 00-2 2v10a2 2 0 002 2zM9 9h6v6H9V9z"/>
            </svg>
            <span>{{ (row.memoryUsage / 1024 / 1024)|number_format(1) }} MB</span>
        </div>
    {% endif %}
    
    {% if row.executionTime %}
        <div class="occurrence-performance__time">
            <svg width="12" height="12" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"/>
            </svg>
            <span>{{ (row.executionTime * 1000)|number_format(0) }} ms</span>
        </div>
    {% endif %}
    
    {% if not row.memoryUsage and not row.executionTime %}
        <span class="text-muted">-</span>
    {% endif %}
</div>